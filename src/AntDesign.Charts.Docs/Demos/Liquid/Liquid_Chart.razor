@page "/Liquid_Chart"

<Liquid Config="config1" />

<Divider>动手实验</Divider>
<DynamicExample @ref="@example">
    <Liquid @ref="@chart" />
</DynamicExample>

@code{

    public IChartComponent chart;
    DynamicExample example;

    protected override void OnAfterRender(bool firstRender)
    {
        base.OnAfterRender(firstRender);
        example.Chart = chart;
    }



    #region 示例1

    LiquidConfig config1 = new LiquidConfig()
    {
        Min = 0,
        Max = 10000,
        Percent = (decimal?)0.25,
    };

    #endregion 示例1

}

